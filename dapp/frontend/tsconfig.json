{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
     "outDir": "./dist",
      "baseUrl": ".",
      "paths": {
      "@/*": ["./src/*"]
    }
     
  },
  "include": ["src", "tailwind.config.cjs"],
  "exclude": ["node_modules"]

}
